@model IEnumerable<SEA_Application.Models.AspNetStudent_PaymentDetail>

@{
    ViewBag.Title = "Index";
}

<h2>Challan Detail</h2>

<div class="box box-primary">
    <div class="box-body">
        <table class="table">
            <tr>
                <th>
                    @Html.DisplayNameFor(model => model.AspNetFinanceLedger.Name)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Amount)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.AspNetStudent_Payment.AspNetUser.Name)
                </th>
                <th></th>
            </tr>
            @foreach (var item in Model)
            {
                <tr>
                    <td>
                        @Html.DisplayFor(modelItem => item.AspNetFinanceLedger.Name)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Amount)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.AspNetStudent_Payment.AspNetUser.Name)
                    </td>
                    
                </tr>
            }
        </table>

    </div>
    <div class="box-footer">
        <button class="btn btn-default" onclick="window.location.href = '/FeeManagement/Student_History'">Back</button>
    </div>
</div>

