    function Basic_Salary() {
        var gross = $("#GrossSalary").val();
        var bs = (2 / 3) * gross;
        bs = parseInt(bs);
        document.getElementById("BasicSalary").value = bs;
    }

    function Medical_Allowance() {
        var basic = $("#BasicSalary").val();
        var ma = (10 / 100) * basic;
        ma = parseInt(ma);
        document.getElementById("MedicalAllowance").value = ma;
    }
    function Accomodationn() {
        var basic = $("#BasicSalary").val();
        var acc = (basic / 100) * 40;
        acc = parseInt(acc);
        document.getElementById("Accomodation").value = acc;

    }


    function checkPhone(phone) {
        //var phone = $("#inputEmail3").val();
        var length = phone.value.length;
        console.log(length);

        if (length == 11) {
            var patt = new RegExp("^[0]+[0-9]{10,}$");
            var test = patt.test(phone.value);
            console.log(test);
            if (test == false) {
                $('#Phone-Error').html("Phone number is not in correct formate");
                $('#FormSubmit').prop('disabled', true);
            } else {
                $('#FormSubmit').prop('disabled', false);
                $('#Phone-Error').html(" ");
            }
        } else {
            $('#Phone-Error').html("Phone number is not in correct formate");
            $('#FormSubmit').prop('disabled', true);
        }
    }
    function onCancel() {
        $('#cancel').click();
    }
    function onSave() {
        $('#FormSubmit').click();
    }
    function redirect() {
        window.location.href = "AccountantIndex";
    }
    function redirectToEdit() {
        var id = $("#ID").val();
        window.location.href = "AccountantEdit?id=" + id;
    }
    function DISABLE() {
        $("#myModal1").modal('show');
    }
    function cnfmDisable() {
        var id = $("#ID").val();
        window.location.href = "/AspNetUser/Delete?id=" + id + "&type=Accountant";
    }
    function Enable() {
        var id = $("#ID").val();
        window.location.href = "/AspNetUser/Enable?id=" + id + "&type=Accountant";
    }
</script>

