<script>

    $(document).ready(function () {

        $('input').prop('readonly', true)
        $('select').prop('disabled', true);

        var nation = $("#Nation").val();
        var relegion = $("#RELEGION").val();
        var gender = $("#GENDER").val();

        $('#Nationality option[value="' + nation + '"]').prop('selected', true);
        $('#Relegion option[value="' + relegion + '"]').prop('selected', true);
        $('#Gender option[value="' + gender + '"]').prop('selected', true);


        $("#birthDate").bind('blur', function () {
            var dob = $("#birthDate").val();
            var dob = dob.split("-");
            var date = new Date();

            var day = date.getDate();
            var month = date.getMonth() + 1;
            var year = date.getFullYear();

            if (dob[0] <= year) {
                $('#FormSubmit').prop('disabled', false);
                $('#DOB-Error').html(" ");
                if (dob[0] == year) {
                    if (dob[1] <= month) {
                        $('#FormSubmit').prop('disabled', false);
                        $('#DOB-Error').html(" ");
                        if (dob[1] == month) {
                            if (dob[2] < day) {
                                $('#FormSubmit').prop('disabled', false);
                                $('#DOB-Error').html(" ");
                            } else {
                                $('#FormSubmit').prop('disabled', true);
                                $('#DOB-Error').html("Enter a date less then today");
                            }
                        } else if (dob[1] > month) {
                            $('#FormSubmit').prop('disabled', true);
                            $('#DOB-Error').html("Enter a date less then today");
                        }
                    } else {
                        $('#FormSubmit').prop('disabled', true);
                        $('#DOB-Error').html("Enter a date less then today");
                    }
                }
            } else {
                $('#FormSubmit').prop('disabled', true);
                $('#DOB-Error').html("Enter a date less then today")
            }

        });
       
        $("#dateAvailable").bind('blur', function () {
            var dob = $("#dateAvailable").val();
            var dob = dob.split("-");

            var date = new Date();
            var day = date.getDate();
            var month = date.getMonth() + 1;
            var year = date.getFullYear();

            if (dob[0] >= year) {
                $('#FormSubmit').prop('disabled', false);
                $('#Avail-Error').html(" ");
                if (dob[0] == year) {
                    if (dob[1] >= month) {
                        $('#FormSubmit').prop('disabled', false);
                        $('#Avail-Error').html(" ");
                        if (dob[1] == month) {
                            if (dob[2] > day) {
                                $('#FormSubmit').prop('disabled', false); // ok
                                $('#Avail-Error').html(" ");
                            } else {
                                $('#FormSubmit').prop('disabled', true);
                                $('#Avail-Error').html("Enter a date greater then today");
                            }
                        } else if (dob[1] < month) {
                            $('#FormSubmit').prop('disabled', true);
                            $('#Avail-Error').html("Enter a date greater then today");
                        }
                    } else {
                        $('#FormSubmit').prop('disabled', true);
                        $('#Avail-Error').html("Enter a date greater then today");
                    }
                }
            } else {
                $('#FormSubmit').prop('disabled', true);
                $('#Avail-Error').html("Enter a date greater then today");
            }

        });

        $("#JoiningDate").bind('blur', function () {
            var dob = $("#JoiningDate").val();
            var dob = dob.split("-");

            var date = new Date();
            var day = date.getDate();
            var month = date.getMonth() + 1;
            var year = date.getFullYear();

            if (dob[0] >= year) {
                $('#FormSubmit').prop('disabled', false);
                $('#join-Error').html(" ");
                if (dob[0] == year) {
                    if (dob[1] >= month) {
                        $('#FormSubmit').prop('disabled', false);
                        $('#join-Error').html(" ");
                        if (dob[1] == month) {
                            if (dob[2] > day) {
                                $('#FormSubmit').prop('disabled', false); // ok
                                $('#join-Error').html(" ");
                            } else {
