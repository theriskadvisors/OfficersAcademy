@model IEnumerable<SEA_Application.Models.AspNetDiscountType>
<link href="~/Content/bootstrap.css" rel="stylesheet" />
<h2 class="orange">Discount Type &nbsp; <i class="fa fa-list" aria-hidden="true"></i></h2>
<p>
    @Html.ActionLink("Create New", "Create", "AspNetDiscountType")
</p>
<div class="box box-primary">
    <div class="box-title"></div>
    <div class="box-body">
        <table class="table table-striped table-bordered">
            <tr>
                <th>
                    @Html.DisplayNameFor(model => model.TypeName)
                </th>
                <th></th>
            </tr>
            @foreach (var item in Model)
            {
                <tr>
                    <td>
                        @Html.DisplayFor(modelItem => item.TypeName)
                    </td>
                    <td>
                        <a href="@Url.Action("Edit","AspNetDiscountType", new { id = item.Id })">Edit</a>|
                        <a href="@Url.Action("Details","AspNetDiscountType", new { id = item.Id })">Detail</a>|
                        <a href="@Url.Action("Delete", "AspNetDiscountType", new { id = item.Id })">Delete</a>
                    </td>
                </tr>
            }
        </table>
    </div>
</div>
