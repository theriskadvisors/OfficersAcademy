
@{
    ViewBag.Title = "Attendance";
}

<script type="text/javascript">
    function Autofoucs()
    {
        document.getElementById("roll").focus();
    }
    function Att() {
        var value = $("#roll").val();
        
       
        //$.ajax({
        //    type: "POST",
        //    url: "/AspNetRoles/Auto_Attendance",
        //    data: { StdId: StdId},
        //    success: function () { }
        //});
        $.ajax({
            type: "POST",
            url: "/AspNetAttendance/Auto_Attendance",
            data: { value: value },
            success: function () { }
        });
    }
</script>
<!DOCTYPE html>

<html lang="en">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <title>Login Form</title>
    <link rel="stylesheet" href="css/style.css">
</head>
<body onload="Autofoucs()">
    <section class="container" style="border:1px solid white;background-color:white;width:40%;height:150px; margin-top:200px">
        <div class="login" style="text-align:center">
            <h1>Attendance</h1>
            <form>
                <input type="text" name="login" id="roll" />

                <input type="submit" onclick="Att()" value="Submit">
            </form>
        </div>


    </section>


</body>
</html>