
<label>@Html.ActionLink("Home", "Dashboard", "Account") > @Html.ActionLink("Assessment Questions Category", "Index", "AspNetAssessment_Questions_Category") > <a href="#"><b>Edit</b></a> </label>
<h4>Edit</h4>

<div class="box box-primary">
    <div class="box-body">
          @model SEA_Application.Models.AspNetAssessment_Questions_Category
        @{
            ViewBag.Title = "Edit";
        }

        @using (Html.BeginForm())
        {
            @Html.AntiForgeryToken()

            <div class="form-horizontal">
                <h4>Assessment_Questions_Category</h4>
                <hr />
                @Html.ValidationSummary(true, "", new { @class = "text-danger" })
                @Html.HiddenFor(model => model.Id)
                <div class="form-group">
                    <label class="control-label col-md-1 col-lg-1">Category Name</label>
                    <div class="col-md-6">
                        @Html.EditorFor(model => model.CategoryName, new { htmlAttributes = new { @class = "form-control" } })
                        @Html.ValidationMessageFor(model => model.CategoryName, "", new { @class = "text-danger" })
                    </div>
                </div>
                <hr />
                <div class="form-group">
                    <div class="col-md-offset-1 col-lg-offset-1 col-md-2">
                        <input type="submit" value="Update" class="btn btn-primary" />
                        
                    </div>
               </div>
                <button onclick="window.location.href='Index'" class="btn btn-default">Cancel</button>
            </div>
        }
  

    </div>
</div>

