@model IEnumerable<SEA_Application.Models.AspNetCurriculum>

@{
    ViewBag.Title = "Index";

}

<h2>Index</h2>
<script src="~/AdminLTE/selectone.js"></script>
<p>
    @Html.ActionLink("Create New", "Create")
</p>
<div class="row">
    <div class="form-three widget-shadow">
        <table class="table table-striped">
            <tr>
                <th>
                    @Html.DisplayNameFor(model => model.CurriculumName)
                </th>
                <th></th>
            </tr>

            @foreach (var item in Model)
            {
                <tr>
                    <td>
                        @Html.DisplayFor(modelItem => item.CurriculumName)
                    </td>
                    <td>
                        @Html.ActionLink("Edit", "Edit", new { id = item.Id }) |
                        @Html.ActionLink("Details", "Edit", new { id = item.Id }) |
                        @Html.ActionLink("Delete", "Delete", new { id = item.Id })
                    </td>
                </tr>
            }

        </table>

</div>
</div>