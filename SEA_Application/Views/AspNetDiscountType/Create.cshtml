@model SEA_Application.Models.AspNetDiscountType
@{
    ViewBag.Title = "Create";
}
<script src="~/AdminLTE/selectone.js"></script>
<script>
    $(document).ready(function () {

        $("#TypeName").prop('required', true);

        $('#cancel').click(function () {
            window.location.href = 'Index';
        });
    });

</script>
<label>@Html.ActionLink("Home", "Dashboard", "Account") > <a href="#" class="sidebar-toggle navigate" data-toggle="offcanvas" style="background-color:#F7F7F7 !important">Fee Management</a> > @Html.ActionLink("Discount Type", "Index", "AspNetDiscountType") > <a href="#"><b>Create</b></a> </label>
<h4>Create</h4>

<div class="box box-primary">
    <div class="box-body">
        @using (Html.BeginForm())
        {
            @Html.AntiForgeryToken()
        <h4>Discount Type</h4>
        <hr />
            <div class="form-horizontal">
                @Html.ValidationSummary(true, "", new { @class = "text-danger" })
                <div class="form-group">
                    <label class="control-label col-md-1">Type Name</label>
                    @*@Html.LabelFor(model => model.TypeName, htmlAttributes: new { @class = "control-label col-md-2" })*@
                    <div class="col-md-6">
                        @Html.EditorFor(model => model.TypeName, new { htmlAttributes = new { @class = "form-control" } })
                        @Html.ValidationMessageFor(model => model.TypeName, "", new { @class = "text-danger" })
                    </div>
                </div>
                <hr />
                <div class="form-group">
                    <div class="col-md-offset-1 col-md-1">
                        <input type="submit" value="Create" class="btn btn-primary" />
                    </div>
                    <div class="col-md-2">
                        <button type="button" id="cancel" class="btn btn-default">Cancel</button>
                    </div>
                </div>
            </div>
        }
    </div>
</div>

