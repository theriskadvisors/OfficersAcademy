@model IEnumerable<SEA_Application.Models.AspNetSubject_Curriculum>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.WeightageValue)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.AspNetCurriculum.CurriculumName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.AspNetSubject.SubjectName)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.WeightageValue)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.AspNetCurriculum.CurriculumName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.AspNetSubject.SubjectName)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
