@model SEA_Application.Models.AspNetNote

@{
    ViewBag.Title = "Delete";
    Layout = "~/Views/Shared/_AdminDashboardLayout.cshtml";
}
<label>@Html.ActionLink("Home", "Dashboard", "Account") > @Html.ActionLink("Notes", "Index", "AspnetNotes") > <a href="#"><b>Delete</b></a> </label>
<h4 class="orange">Notes Details </h4>

<h2>Delete Notes</h2>

<div class="box box-primary">
    <div class="box-body">


        <div class="form-horizontal">
            <h4>Notes</h4>
            <script>
                $(document).ready(function () {
                    $('input').prop('disabled', true);
                    $('select').prop('disabled', true);
                })
            </script>

            <hr />

            <div class="form-group">
                <label class="control-label col-md-2"> @Html.DisplayNameFor(model => model.Title):</label>
                <div class="col-md-4">
                    @Html.EditorFor(model => model.Title, new { htmlAttributes = new { @class = "form-control" } })
                    @Html.ValidationMessageFor(model => model.Title, "", new { @class = "text-danger" })
                </div>
            </div>
            <div class="form-group">
                <label class="control-label col-md-2"> @Html.DisplayNameFor(model => model.Description):</label>
                <div class="col-md-4">
                    @Html.EditorFor(model => model.Description, new { htmlAttributes = new { @class = "form-control" } })
                    @Html.ValidationMessageFor(model => model.Description, "", new { @class = "text-danger" })
                </div>
            </div>
            <div class="form-group">
                <label class="control-label col-md-2">model.CourseType)</label>
                <div class="col-md-4">
                    @Html.EditorFor(model => model.CourseType, new { htmlAttributes = new { @class = "form-control" } })
                    @Html.ValidationMessageFor(model => model.CourseType, "", new { @class = "text-danger" })
                </div>
            </div>
            <div class="form-group">
                <label class="control-label col-md-2">model.Price):</label>
                <div class="col-md-4">
                    @Html.EditorFor(model => model.Price, new { htmlAttributes = new { @class = "form-control" } })
                    @Html.ValidationMessageFor(model => model.Price, "", new { @class = "text-danger" })
                </div>
            </div>
            <div class="form-group">
                <label class="control-label col-md-2">Creation Date</label>
                <div class="col-md-4">
                    @Html.EditorFor(model => model.CreationDate, new { htmlAttributes = new { @class = "form-control" } })
                    @Html.ValidationMessageFor(model => model.CreationDate, "", new { @class = "text-danger" })
                </div>
            </div>
            <div class="form-group">
                <label class="control-label col-md-2">Subject Name</label>
                <div class="col-md-4">
                    <div class="checkbox">
                        @Html.EditorFor(model => model.AspNetSubject.SubjectName, new { htmlAttributes = new { @class = "form-control" } })
                        @Html.ValidationMessageFor(model => model.AspNetSubject.SubjectName, "", new { @class = "text-danger" })
                    </div>
                </div>
            </div>


            <div class="form-group">
                @using (Html.BeginForm())
                {
                    @Html.AntiForgeryToken()

                    <div class="col-md-offset-2 col-md-10">

                        <button type="submit" class="btn btn-primary">Delete</button>
                        <button type="button" id="cancel" class="btn btn-default">Back</button>

                    </div>
                }
            </div>

        </div>
    </div>
</div>
<script>
    $(document).ready(function () {


        $("#cancel").click(function () {

            window.location.href = '/AspNetNotes/index';

        });

    })

</script>