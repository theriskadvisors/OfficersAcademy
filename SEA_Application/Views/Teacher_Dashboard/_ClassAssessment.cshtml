                        $('#Evaluation_Question_Table').append($('<tr>').attr("id", Data.Id).click(Data, Reportit).data(Data).append(dfdf))
                        $('#Evaluation_Question_Table').append($('<tr>').append($('<td>').attr("id", "eeniart" + Data.Id).attr("colspan", "2")))
                    }
                    $('#TC').val(teacherComment);
                    $('#PC').val(parentComment);
                    $('#PrC').val(principalcomment);
                }
                else {

                }



                //$('#Evaluation_data').html(data);
                $('Comment_Evaluation').prop('disabled', false);
            },
            error: function (xhr) {
                debugger;
                alert(xhr.responseText);
            }
        });
        printpreview();
    }

