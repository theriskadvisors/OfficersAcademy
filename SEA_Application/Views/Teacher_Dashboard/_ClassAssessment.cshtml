        $("#trId").val(name.id);

        var trrmid = $("#TermID").val();

        $.ajax({
            type: "POST",
            url: "/TermAssessment/ClassEvaluations",
            data: { 'StudentId': item.id, "type": "teacher", TermId: trrmid },
            success: function (data) {
                $('.eeniart0017').css("display", "none");
                if (data.status == "success")
                {
                    evalutionfrom();
                    var classname = data.classname;
                    var studentname = data.studentname;
                    var teachername = data.teachername;

                    var termid = data.TId;
                    var subjects = data.Value
                    var teacherComment = data.teacherComent;
                    var parentComment=data.parentComment;
                    var principalcomment=data.principalcomment;
                    $('#Evaluation_data').html('');

                    $("#className").html(classname+' Progress Report');
                    $("#stdname").html(studentname);
                    $("#teachername").html(teachername);

                    $("#pclassName").html(classname + ' Progress Report');
                    $("#pstdname").html(studentname);
                    $("#pteachername").html(teachername);

                    var table = $('<table class="table" id="Evaluation_Question_Table"><tr style="display:none"><th>Questions</th><th>Assessment</th></tr></table>');
                    $('#Evaluation_data').append(table);
                    for (var i = 0; i < subjects.length; i++)
                    {
                        var Data = subjects[i];
                        var dfdf = $('<td class="text-bold"><h3>' + Data.SubjectName + '</h3></td><td style="float:right"><button class="btn btn-primary save_comment">Assessment Report</button></td>')
