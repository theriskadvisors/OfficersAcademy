    $(document).ready(function () {


              var selectedStudent = '@Model.Id';

      
        $.get('@Url.Action("TeacherClass")',

            { id: selectedStudent }, function (data) {
                selectedClass = data;

                $('#ClassID').val(data);

        })

        //debugger;
        var Na = $('#NATIONALITY').val();
        var re = $('#RELEIGON').val();
        var ge = $('#GENDER').val();
        var De = $('#Degree').val();

        $('#Nationality option[value="'+ Na+'"]').prop('selected', true);
        $('#Religon option[value="' + re + '"]').prop('selected', true);
        $('#Gender option[value="' + ge + '"]').prop('selected', true);
        $('#Degree option[value="' + De + '"]').prop('selected', true);

        $('#GrossSalary').change(function () {
            Basic_Salary();
            Medical_Allowance();
            Accomodationn();
        });



        $('#cancel').click(function () {
            window.location.href = '@Url.Action("TeachersIndex", "AspNetUser")';
        });

        $("#UserName").bind('blur', function () {
            var userName = $("#UserName").val();

            $.ajax({
                type: "POST",
                url: "/AspNetUser/GetUserName",
                data: { "userName": userName },
                success: function (data) {
                    if (data == 0){
                        $('#FormSubmit').removeAttr('disabled');
                    }
                    else {
                        $('#UserName-Error').html("UserName Already taken");
                        $('#FormSubmit').prop('disabled', true);
                    }

                }
            });

        });

        $("#Email").bind('blur', function () {
            var email = $("#Email").val();

            $.ajax({
                type: "POST",
                url: "/AspNetUser/Email",
                data: { "Email": email },
                success: function (data) {
                    console.log(data);
                    if (data == 0) {
                        $('#FormSubmit').prop('disabled', false);
                    }
                    else {
                        $('#Email-Error').html("Email Already taken");

                        $('#FormSubmit').prop('disabled', true);
                    }

                }
            });

        });

        $("#birthDate").bind('blur', function () {
            var dob = $("#birthDate").val();
            var dob = dob.split("-");
            var date = new Date();

            var day = date.getDate();
            var month = date.getMonth() + 1;
            var year = date.getFullYear();

            if (dob[0] <= year) {
                $('#FormSubmit').prop('disabled', false);
                $('#DOB-Error').html(" ");
                if (dob[0] == year) {
                    if (dob[1] <= month) {
                        $('#FormSubmit').prop('disabled', false);
                        $('#DOB-Error').html(" ");
                        if (dob[1] == month) {
                            if (dob[2] < day) {
                                $('#FormSubmit').prop('disabled', false);
                                $('#DOB-Error').html(" ");
                            } else {
                                $('#FormSubmit').prop('disabled', true);
                                $('#DOB-Error').html("Enter a date less than today");
                            }
                        } else if (dob[1] > month) {
                            $('#FormSubmit').prop('disabled', true);
                            $('#DOB-Error').html("Enter a date less than today");
                        }
                    } else {
                        $('#FormSubmit').prop('disabled', true);
                        $('#DOB-Error').html("Enter a date less than today");
                    }
                }
            } else {
                $('#FormSubmit').prop('disabled', true);
                $('#DOB-Error').html("Enter a date less than today")
            }

        });

        $("#dateAvailable").bind('blur', function () {
            var dob = $("#dateAvailable").val();
            var dob = dob.split("-");

            var date = new Date();
            var day = date.getDate();
            var month = date.getMonth() + 1;
            var year = date.getFullYear();

            if (dob[0] >= year) {
                $('#FormSubmit').prop('disabled', false);
                $('#Avail-Error').html(" ");
                if (dob[0] == year) {
                    if (dob[1] >= month) {
                        $('#FormSubmit').prop('disabled', false);
                        $('#Avail-Error').html(" ");
                        if (dob[1] == month) {
                            if (dob[2] >= day) {
                                $('#FormSubmit').prop('disabled', false); // ok
                                $('#Avail-Error').html(" ");
                            } else {
                                $('#FormSubmit').prop('disabled', true);
                                $('#Avail-Error').html("Enter a date greater than today");
                            }
                        } else if (dob[1] < month) {
                            $('#FormSubmit').prop('disabled', true);
                            $('#Avail-Error').html("Enter a date greater than today");
                        }
                    } else {
                        $('#FormSubmit').prop('disabled', true);
                        $('#Avail-Error').html("Enter a date greater than today");
                    }
                }
            } else {
                $('#FormSubmit').prop('disabled', true);
                $('#Avail-Error').html("Enter a date greater than today");
            }

        });

        $("#JoiningDate").bind('blur', function () {
            var dob = $("#JoiningDate").val();
            var dob = dob.split("-");

            var date = new Date();
            var day = date.getDate();
            var month = date.getMonth() + 1;
            var year = date.getFullYear();

            if (dob[0] >= year) {
                $('#FormSubmit').prop('disabled', false);
                $('#join-Error').html(" ");
                if (dob[0] == year) {
                    if (dob[1] >= month) {
                        $('#FormSubmit').prop('disabled', false);
                        $('#join-Error').html(" ");
                        if (dob[1] == month) {
                            if (dob[2] >= day) {
                                $('#FormSubmit').prop('disabled', false); // ok
                                $('#join-Error').html(" ");
                            } else {
                                $('#FormSubmit').prop('disabled', true);
                                $('#join-Error').html("Enter a date greater than today");
                            }
                        } else if (dob[1] < month) {
                            $('#FormSubmit').prop('disabled', true);
                            $('#join-Error').html("Enter a date greater than today");
                        }
                    } else {
                        $('#FormSubmit').prop('disabled', true);
                        $('#join-Error').html("Enter a date greater than today");
                    }
                }
            } else {
                $('#FormSubmit').prop('disabled', true);
                $('#join-Error').html("Enter a date greater than today");
            }
        });

    });

    function Basic_Salary() {
        var gross = $("#GrossSalary").val();
        var bs = (2 / 3) * gross;
        bs = parseInt(bs);
        document.getElementById("BasicSalary").value = bs;
    }

    function Medical_Allowance() {
        var basic = $("#BasicSalary").val();
        var ma = (10 / 100) * basic;
        ma = parseInt(ma);
        document.getElementById("MedicalAllowance").value = ma;
    }
    function Accomodationn() {
        var basic = $("#BasicSalary").val();
        var acc = (basic / 100) * 40;
        acc = parseInt(acc);
        document.getElementById("Accomodation").value = acc;

    }
    function onCancel() {
        $('#cancel').click();
    }
    function onSave() {
        $('#FormSubmit').click();
    }

    function checkPhone(phone) {
        var length = phone.value.length;

        if (length == 11) {
            var patt = new RegExp("^[0]+[0-9]{10,}$");
            var test = patt.test(phone.value);
            console.log(test);
            if (test == false) {
                $('#Phone-Error').html("Phone number is not in correct formate");
                $('#FormSubmit').prop('disabled', true);
            } else {
                $('#FormSubmit').prop('disabled', false);
                $('#Phone-Error').html(" ");
            }
        } else {
            $('#Phone-Error').html("Phone number is not in correct formate");
            $('#FormSubmit').prop('disabled', true);
        }
    }


    
