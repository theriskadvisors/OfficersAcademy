

<script src="~/AdminLTE/selectone.js"></script>

<label>@Html.ActionLink("Home", "Dashboard", "Account") > <a href="#" class="sidebar-toggle navigate" data-toggle="offcanvas" style="background-color:#F7F7F7 !important">User</a> > @Html.ActionLink("Fee Type", "Index", "AspNetFeeType") > <a href="#"><b>Create</b></a> </label>
<h4>Create</h4>
<div class="box box-primary">
    <div class="box-body">
        @model SEA_Application.Models.AspNetFeeType

        @{
            ViewBag.Title = "Create";
        }

        <script>
            $(document).ready(function () {
                $("#LedgerID").prop('required', true);
            });
        </script>
        <h4>Fee Type</h4>
        <hr />
        


        @using (Html.BeginForm())
        {
            @Html.AntiForgeryToken()

            <div class="form-horizontal">
               
                @Html.ValidationSummary(true, "", new { @class = "text-danger" })
                <div class="form-group">
                    <label class="control-label col-md-2">Ledger</label>
                    <div class="col-md-6">
                        @Html.DropDownList("LedgerID", null, htmlAttributes: new { @class = "form-control" })
                        @Html.ValidationMessageFor(model => model.LedgerID, "", new { @class = "text-danger" })
                    </div>
                </div>

                <div class="form-group">
                    <div class="col-md-offset-2 col-md-2">
                        <input type="submit" value="Create" class="btn btn-primary" />
                    </div>
                    <div class="col-md-2">
                        <button class="btn btn-danger" onclick="window.location.href = 'Index'">Cancel</button>
                    </div>
                </div>
            </div>
        }




    </div>
</div>

