@model SEA_Application.Models.AspNetFeeType
@{
    ViewBag.Title = "Edit";
}
<label>@Html.ActionLink("Home", "Dashboard", "Account") > <a href="#" class="sidebar-toggle navigate" data-toggle="offcanvas" style="background-color:#F7F7F7 !important">User</a> > @Html.ActionLink("Fee Type", "Index", "AspNetFeeType") > <a href="#"><b>Edit</b></a> </label>
<h4>Edit Fee</h4>
<div class="box box-primary">
    <div class="box-body ">


        @using (Html.BeginForm())
        {
            @Html.AntiForgeryToken()

            <div class="form-horizontal">
                <h4>Fee Type</h4>
                <hr />
                @Html.ValidationSummary(true, "", new { @class = "text-danger" })
                @Html.HiddenFor(model => model.Id)
                <div class="form-group">
                    <label class="control-label col-md-1">Ledger</label>
                    <div class="col-md-6">
                        @Html.DropDownList("LedgerID", null, htmlAttributes: new { @class = "form-control" })
                        @Html.ValidationMessageFor(model => model.LedgerID, "", new { @class = "text-danger" })
                    </div>
                </div>
                <hr />
                <div class="form-group">
                    
                    <div class="col-md-offset-1 col-md-1">
                        <input type="submit" value="Update" class="btn btn-primary" />
                    </div>
                    <div class="col-md-1">
                        <button class="btn btn-default" onclick="window.location.href = 'Index'">Cancel</button>
                    </div>
                </div>
            </div>
        }
    </div>
</div>