@model IEnumerable<SEA_Application.Models.AspNetFinanceVoucherDetail>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<div class="box box-primary">
    <div class="box-title"></div>
    <div class="box-body">

        <p>
            @Html.ActionLink("Create New", "Create")
        </p>
        <table class="table">
            <tr>
                <th>
                    @Html.DisplayNameFor(model => model.Credit)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Debit)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.TransactionDescription)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.AspNetFinanceLedger.Code)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.AspNetFinanceLedgerType.Name)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.AspNetFinanceVoucher.Status)
                </th>
                <th></th>
            </tr>

            @foreach (var item in Model)
            {
                <tr>
                    <td>
                        @Html.DisplayFor(modelItem => item.Credit)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Debit)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.TransactionDescription)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.AspNetFinanceLedger.Code)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.AspNetFinanceLedgerType.Name)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.AspNetFinanceVoucher.Status)
                    </td>
                    <td>
                        @Html.ActionLink("Edit", "Edit", new { id = item.Id }) |
                        @Html.ActionLink("Details", "Edit", new { id = item.Id }) |
                        @Html.ActionLink("Delete", "Delete", new { id = item.Id })
                    </td>
                </tr>
            }

        </table>
    </div>
</div>